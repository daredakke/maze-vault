config_version=5

[application]

config/name="nanoban"
config/version="1.0.0.0"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://assets/nanoban-icon-default.png"
config/windows_native_icon="res://nanoban-icon.ico"

[autoload]

Global="*res://scripts/autoload/global.gd"
EventBus="*res://scripts/autoload/event_bus.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=176
window/size/resizable=false
window/size/window_width_override=640
window/size/window_height_override=352
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[file_customization]

folder_colors={
"res://assets/": "blue",
"res://resources/": "green",
"res://scenes/": "yellow",
"res://scripts/": "red"
}

